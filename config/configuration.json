{
  "general": {
    "events": [
      "HandStart",
      "FirstDigitTouch",
      "BothStartLoadPhase",
      "LiftOff",
      "Replace",
      "BothReleased"
    ],
    "maximum-paralel-jobs": 10,
    "subsamples": 250,
    "cross-validation-folds": 8,
    "dataset-path": "../dataset/train/",
    "trained-models-path": "../models/",
    "trained-extractors-path": "../extractors/",
    "submission-path": "../submission/",
    "result-path": "../results/results.csv",
    "subject-range": {
      "start": 1,
      "end": 2
    },
