    "event-window-after": 2,
    "preload-model-file": "_csp_20210905113839_.sav",
    "should-train": false,
    "should-save": true,
    "smoothing-window-size": 200,
    "smoothing-type": "boxcar",
    "number-of-filters": 20,
    "regularization": "ledoit_wolf",
    "multiple-subjects": {
      "should-use-same": true,
      "should-train": true
    }
  },
  "classifier": {
    "type": "support-vector-machine",
    "preload-model-file": null,
    "should-train": true,
    "should-save": true,
    "multiple-subjects": {
      "should-use-same-classifier": true,
      "should-train": true
    }
  }
}